{
  "name": "douban",
  "version": "1.1.0",
  "description": "The extension for douban.",
  "manifest_version": 3,
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.douban.com/subject/*"
      ],
      "js": [
        "douban.js"
      ],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "https://*.douban.com/subject/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{a73808b1-96a4-41ae-9dad-7888c4b9235c}"
    }
  }
}